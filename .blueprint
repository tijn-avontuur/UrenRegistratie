created:
    - database/factories/UserFactory.php
    - database/factories/ProjectFactory.php
    - database/factories/TimeEntryFactory.php
    - database/factories/ProjectUserFactory.php
    - database/factories/ProjectAttachmentFactory.php
    - database/migrations/2026_02_05_132419_create_users_table.php
    - database/migrations/2026_02_05_132420_create_projects_table.php
    - database/migrations/2026_02_05_132421_create_time_entries_table.php
    - database/migrations/2026_02_05_132422_create_project_users_table.php
    - database/migrations/2026_02_05_132423_create_project_attachments_table.php
    - database/migrations/2026_02_05_132424_create_project_user_table.php
    - app/Models/User.php
    - app/Models/Project.php
    - app/Models/TimeEntry.php
    - app/Models/ProjectUser.php
    - app/Models/ProjectAttachment.php
models:
    User: { name: string, email: 'string unique', password: string, role: string, relationships: { hasMany: TimeEntry, belongsToMany: Project }, timestamps: timestamps }
    Project: { title: string, description: 'text nullable', color: 'string nullable', relationships: { hasMany: 'TimeEntry, ProjectAttachment', belongsToMany: User }, timestamps: timestamps }
    TimeEntry: { user_id: 'id foreign:users', project_id: 'id foreign:projects', date: date, start_time: datetime, end_time: 'datetime nullable', duration_minutes: 'integer nullable', source: string, relationships: { belongsTo: 'User, Project' }, timestamps: timestamps }
    ProjectUser: { user_id: 'id foreign:users', project_id: 'id foreign:projects', timestamps: timestamps }
    ProjectAttachment: { project_id: 'id foreign:projects', filename: string, filepath: string, uploaded_at: timestamp, relationships: { belongsTo: Project } }
